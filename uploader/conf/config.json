{
    "server":
    {
        "protocol": "https",
        "host": "synapse-dev.isrd.isi.edu",
        "catalog_id": 1
    },
    "asset_mappings":
    [
        {
            "file_pattern":"Bhv.*",
            "ext_pattern":".*[.](?P<ext>m4v)$",
            "synapse_file_type": "behavior movie",
            "checksum_types": ["md5"]
        },
	{
            "file_pattern":"Img.*",
            "ext_pattern":".*[.](?P<ext>ome[.]tiff+)$",
            "synapse_file_type": "spim image",
            "checksum_types": ["md5"]
        },
	{
            "file_pattern":"CropImg.*",
            "ext_pattern":".*[.](?P<ext>ome[.]tiff+)$",
            "synapse_file_type": "cropped image",
            "checksum_types": ["md5"]
        },
	{
            "file_pattern":"(Roi|Crop|Syn).*(segments|synapses)",
            "ext_pattern":".*[.](?P<ext>csv)$",
            "synapse_file_type": "synapse list",
            "checksum_types": ["md5"]
        },
	{
            "file_pattern":"(Roi|Crop|Nuc).*nuclei",
            "ext_pattern":".*[.](?P<ext>csv)$",
            "synapse_file_type": "nucleus list",
            "checksum_types": ["md5"]
        }
    ]

}
